{
  "tooltip": {
    "extraCssText": "box-shadow: 0px 2px 14px 0px rgba(0, 0, 0, 0.15);",
    "show": true,
    "backgroundColor": "rgba(53, 53, 53, 0.9)",
    "borderColor": "#344158",
    "borderWidth": 1,
    "textStyle": { "color": "#fff", "fontSize": "12", "lineHeight": 26 },
    "axisPointer": {
      "lineStyle": {
        "color": "#C8C8C8"
      }
    },
    "padding": [6],
    "trigger": "axis"
  },
  "legend": {
    "show": true,
    "bottom": "0",
    "textStyle": {
      "color": "#999999"
    },
    "itemWidth": 14,
    "itemHeight": 8
  },
  "line": {
    "areaStyle": {
      "color": {
        "type": "linear",
        "x": 0.6,
        "y": 1,
        "x2": 0.6,
        "y2": 0.3,
        "colorStops": [{
            "offset": 0, "color": "#242424" 
        }, {
          "offset": 1, "color": "#224056" 
        }],
        "global": false 
      },
      "series": {
        "areaStyle": {
          "color": {
            "type": "linear",
            "x": 0.6,
            "y": 1,
            "x2": 0.6,
            "y2": 0.3,
            "colorStops": [{
                "offset": 0, "color": "red" 
            }, {
              "offset": 1, "color": "#224056" 
            }],
            "global": false 
          }
        }
      }
    }
  },
  "bar": {
    "showBackground": false,
    "backgroundStyle": {
        "color": "rgba(180, 180, 180, 1)"
    },
    "areaStyle": {
      "color": {
        "type": "linear",
        "x": 0.6,
        "y": 1,
        "x2": 0.6,
        "y2": 0.3,
        "colorStops": [{
            "offset": 0, "color": "#242424" 
        }, {
          "offset": 1, "color": "#224056" 
        }],
        "global": false 
      }
    }
  },
  "categoryAxis": {
    "axisLine": {
      "show": true,
      "lineStyle": {
        "color": "#666666"
      }
    },
    "axisTick": {
      "show": true,
      "lineStyle": {
        "color": "#666666"
      }
    },
    "axisLabel": {
      "show": true,
      "textStyle": {
        "color": "#aeaeae",
        "fontSize": "12"
      }
    },
    "splitLine": {
      "show": true,
      "lineStyle": {
        "color": ["#666666"]
      }
    },
    "splitArea": {
      "show": false,
      "areaStyle": {
        "color": ["rgba(250,250,250,0.05)", "rgba(200,200,200,0.02)"]
      }
    }
  },
  "valueAxis": {
    "axisLine": {
      "show": true,
      "lineStyle": {
        "color": "#666666"
      }
    },
    "axisTick": {
      "show": true,
      "lineStyle": {
        "color": "#666666"
      }
    },
    "axisLabel": {
      "show": true,
      "textStyle": {
        "color": "#aeaeae",
        "fontSize": "12"
      }
    },
    "splitLine": {
      "show": true,
      "lineStyle": {
        "color": ["#666666"]
      }
    }
  },
  "map": {
    "itemStyle": {
      "normal": {
        "areaColor": "red",
        "borderColor": "green",
        "borderWidth": 0.5
      },
      "emphasis": {
        "areaColor": "green",
        "borderColor": "#585858",
        "borderWidth": 0.5
      }
    },
    "label": {
      "normal": {
        "textStyle": {
          "color": "#4a4a4a"
        }
      },
      "emphasis": {
        "textStyle": {
          "color": "yellow"
        }
      }
    }
  },
  "pie": {
    "legend": {
      "textStyle": {
        "color": "#ffffff"
      }
    }
  },
  "geo": {
    "itemStyle": {
      "normal": {
        "areaColor": "#303335",
        "borderColor": "#202224",
        "borderWidth": 0.5
      },
      "emphasis": {
        "areaColor": "#4a4a4a",
        "borderColor": "#202224",
        "borderWidth": 1.5
      }
    },
    "label": {
      "normal": {
        "textStyle": {
          "color": "#ffffff"
        }
      },
      "emphasis": {
        "textStyle": {
          "color": "#1890FF"
        }
      }
    }
  },
  "effectScatter": {
    "label": {
      "color": "#ffffff",
      "backgroundColor": "rgba(0, 0, 0, 1)",
      "borderRadius": 8,
      "fontSize": 12
    }
  },
  "markPoint": {
    "label": {
      "normal": {
        "textStyle": {
          "color": "#ffffff"
        }
      },
      "emphasis": {
        "textStyle": {
          "color": "#ffffff"
        }
      }
    }
  }
}
  